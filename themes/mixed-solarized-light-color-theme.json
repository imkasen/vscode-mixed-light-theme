{
    "name": "Mixed Solarized Light",
    "type": "light",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "enumMember": {
            "foreground": "#00978A"
        },
        "module": {
            "foreground": "#C25D1E"
        },
    },
    "tokenColors": [
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#93A1A1",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage", // e.g. 'def'ï¼Œ'nonlocal' in python
            ],
            "settings": {
                "foreground": "#8762C6",
            }
        },
        {
            "name": "Keyword Other, Number Suffix",
            "scope": [
                "keyword.other.unit", // e.g. '3.14159f' in c
                "storage.type.numeric" // e.g. '123n' in js
            ],
            "settings": {
                "foreground": "#009C8F",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operator, Punctuation",
            "scope": [
                "keyword.operator",
                "punctuation",
                "entity.name.function.operator", // e.g. '<<' in cpp
                "constant.other.color", // e.g. '#f0f0f0' in css
            ],
            "settings": {
                "foreground": "#2AA198",
                "fontStyle": "" // make sure it has no font style
            }
        },
        {
            "name": "Python @ Decorator",
            "scope": [
                "punctuation.definition.decorator.python",
            ],
            "settings": {
                "foreground": "#268BD2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Punctuation #", // e.g. '#include'
            "scope": [
                "punctuation.definition.directive.c",
                "punctuation.definition.directive.cpp",
                "punctuation.definition.annotation.java"
            ],
            "settings": {
                "foreground": "#8762C6"
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape" // e.g. '\n'
            ],
            "settings": {
                "foreground": "#2AA198",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#53676D",
                "fontStyle": "" // make sure it has no font style
            }
        },
        {
            "name": "Variable Language",
            "scope": [
                "variable.language" // e.g. '*' of 'java.util.*' in java
            ],
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "Other Variable",
            "scope": [
                "support.other.variable", // e.g. 'console' in js
            ],
            "settings": {
                "foreground": "#CF4B56"
            }
        },
        {
            "name": "Parameter",
            "scope": [
                "variable.parameter",
            ],
            "settings": {
                "foreground": "#657B83",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Python Self Parameter in Class",
            "scope": "variable.parameter.function.language.special.self.python",
            "settings": {
                "foreground": "#C25D1E",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Number, Constant",
            "scope": [
                "constant",
                "variable.other.constant", // constant variable
                "support.constant", // e.g. 'sans-serif' in css
            ],
            "settings": {
                "foreground": "#CB4B16"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
            ],
            "settings": {
                "foreground": "#489100"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#2AA198"
            }
        },
        {
            "name": "String Placeholder", // e.g. '%d' '%s' in string
            "scope": [
                "constant.character",
                "constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#CB4B16"
            }
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function",
                "variable.function",
            ],
            "settings": {
                "foreground": "#268BD2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Support Function",
            "scope": [
                "support.function", // e.g. 'range' in python
                "support.function.magic" // e.g. '__init__' in python
            ],
            "settings": {
                "foreground": "#009C8F",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Support Function JS",
            "scope": [
                "support.function.js", // e.g. 'Symbol()' in js
            ],
            "settings": {
                "foreground": "#268BD2",
            }
        },
        {
            "name": "Class",
            "scope": [
                "entity.name.class",
                "entity.name.type.class",
                "support.type",
                "support.class",
            ],
            "settings": {
                "foreground": "#B58900"
            }
        },
        {
            "name": "Type, Namespaces",
            "scope": [
                "entity.name.type",
                "entity.name.namespace" // e.g. namespace variable in cpp
            ],
            "settings": {
                "foreground": "#B58900"
            }
        },
        {
            "name": "Enum Java",
            "scope": [
                "constant.other.enum.java"
            ],
            "settings": {
                "foreground": "#00978A"
            }
        },
        {
            "name": "Tag in HTML CSS",
            "scope": [
                "entity.name.tag",
            ],
            "settings": {
                "foreground": "#CF4B56"
            }
        },
        {
            "name": "Attributes in HTML CSS",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#6C71C4"
            }
        },
        {
            "name": "Text in HTML",
            "scope": [
                "text.html.derivative", // e.g. texts in elements
            ],
            "settings": {
                "foreground": "#53676D"
            }
        },
        {
            "name": "Markdown Heading Text",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#489100",
            }
        },
        {
            "name": "Markdown Heading Punctuation #",
            "scope": [
                "punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#CF4B56"
            }
        },
        {
            "name": "Text in Markdown",
            "scope": [
                "text.html.markdown"
            ],
            "settings": {
                "foreground": "#53676D"
            }
        },
        {
            "name": "Markdown List Punctuation", // e.g. '-', '1.'
            "scope": [
                "punctuation.definition.list.begin.markdown"
            ],
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Markdown Link Title", // [title]()
            "scope": [
                "string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Markdown Image Link Title", // ![title]()
            "scope": [
                "string.other.link.description.markdown"
            ],
            "settings": {
                "foreground": "#CF4B56"
            }
        },
        {
            "name": "Markdown Link",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "foreground": "#489100",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italics",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Code Block Language Tag", // e.g. '``` js'
            "scope": [
                "fenced_code.block.language.markdown"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Quote", // > ...
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#93A1A1",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Inline Code", // '`print("hello")`'
            "scope": [
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#CB4B16"
            }
        },
        {
            "name": "Markdown Separator", // '---'
            "scope": [
                "meta.separator"
            ],
            "settings": {
                "foreground": "#93A1A1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "INI Title",
            "scope": [
                "entity.name.section.group-title.ini"
            ],
            "settings": {
                "foreground": "#489100",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Value in INI",
            "scope": [
                "source.ini"
            ],
            "settings": {
                "foreground": "#53676D",
            }
        },
        {
            "name": "Value in Ignore",
            "scope": [
                "source.ignore"
            ],
            "settings": {
                "foreground": "#53676D",
            }
        },
    ],
